
<h2>Create Newsletter</h2>

<span class="adminmenu"><%= link_to 'Back', :controller => 'admin/newsletter', :action => 'list' %></span>


<%= error_messages_for :newsletter %>
<% form_for :newsletter  do |f| -%>
