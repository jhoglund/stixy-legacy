<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	
	<style>
	</style>
</head>
<body>
<%= pagination_links_remote(@pages, @page, {:action => 'board_list_detailed_ajax'}, {:update => 'board_list'}) %>
<br>
<%= @page.number %>
<%= link_to("Prev", :controller => "dev", :action => "paginator", :page => @page.prev.number) if @page.prev? %>
<%= link_to("Next", :controller => "dev", :action => "paginator",:page => @page.next.number) if @page.next? %>
<% @page.each do |board| %>
	<%= board.id %> | <%= board.updated_by.display_name %>
	<hr>
<% end %>	
</body>
</html>