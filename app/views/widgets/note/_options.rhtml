<div class="opt-panel wopt wopt-deactivated" id="wopt-note">
	<span class="opt-tab">
	  <div class="left"><div>&nbsp;</div></div>
	  <div class="right"><div>&nbsp;</div></div>
		<div class="label">Note options</div>
	</span>
	<div class="wopt-locked-panel">&nbsp;</div>
	<div class="opt-scroll wopt-locked-ghosted">
		<div class="opt-content">
      <h4>Note Style</h4>
      <!-- <p class="w-n-clone">Clone</p> -->
			<div class="opt-themes" id="widget_note_opt_themes">
				<div class="opt-theme-spacer">&nbsp;</div>
				<div class="wopt-theme opt-theme wopt-theme-1" id="wopt-theme-1"><span class="label">Indian</span></div>
        <div class="wopt-theme opt-theme wopt-theme-2" id="wopt-theme-2"><span class="label">Spring</span></div>
        <div class="wopt-theme opt-theme wopt-theme-3" id="wopt-theme-3"><span class="label">Caribbean</span></div>
        <div class="wopt-theme opt-theme wopt-theme-4" id="wopt-theme-4"><span class="label">New Haven</span></div>
			</div>
			
			<div class="deactivated opt-row" style="padding-top:6px;">
        <div class="opt-devider"></div>
        <h4>Link to web page</h4>
        <div class="opt-row" style="padding-top:6px;">
          <%= link_to_function("Insert link", nil, :button => {:type => "ghosted"}, :class => "button-label  insert-link button-icon", :id => 'link_text') %>
        </div>
       </div>
      
